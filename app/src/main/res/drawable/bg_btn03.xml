<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android">

    <!--
        stroke 设置外框线宽度 - 厚度
        dashGap 设置虚线间隔 - 每个虚线段的距离
        dashWidth 设置虚线宽度 - 每条虚线的长度
    -->
    <stroke
            android:dashGap="15dp"
            android:dashWidth="20dp"
            android:width="2dp"
            android:color="#ffb3b3"/>

    <corners
            android:radius="50dp"/>

</shape>